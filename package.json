{
  "name": "gyazo-api",
  "private": false,
  "version": "0.3.0",
  "description": "API Client for Gyazo.com",
  "main": "lib/gyazo-api.js",
  "scripts": {
    "test": "run-s test:*",
    "test:mocha": "mocha tests/test_*.es6 --compilers js:babel-register",
    "test:eslint": "eslint *.es6 src/*.es6 tests/*.es6",
    "build": "run-s build:*",
    "build:babel": "babel src/ --out-dir lib/ --source-maps inline",
    "watch": "run-p watch:*",
    "watch:babel": "npm run build:babel -- --watch",
    "watch:eslint": "chokidar '(src|test)/*.es6' -c 'npm run test:eslint'"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.4.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-if-in-test": "^0.1.2",
    "mocha": "^2.3.4",
    "npm-run-all": "^3.1.1"
  },
  "dependencies": {
    "request": "*"
  },
  "keywords": [
    "gyazo",
    "image",
    "upload"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shokai/node-gyazo-api.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/node-gyazo-api/issues"
  },
  "homepage": "https://github.com/shokai/node-gyazo-api"
}
